{
  "name": "fly.js",
  "version": "1.0.0",
  "description": "A JavaScript SDK for the Concourse CI API.",
  "main": "lib/index.js",
  "scripts": {
    "start": "babel -s inline -d lib -w src",
    "build": "babel -s inline -d lib src",
    "prepublish": "npm test && npm run build",
    "test": "mocha --compilers js:babel-core/register ./test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/infrablocks/fly.js.git"
  },
  "keywords": [
    "concourse",
    "ci",
    "javascript",
    "sdk"
  ],
  "author": "Toby Clemson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/infrablocks/fly.js/issues"
  },
  "homepage": "https://github.com/infrablocks/fly.js#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "mocha": "^5.2.0"
  }
}
